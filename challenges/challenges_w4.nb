Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"isKap", "[", "x_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"numAsList", "=", 
       RowBox[{"Characters", "[", 
        RowBox[{"ToString", "[", 
         RowBox[{"x", "^", "2"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"firstHalf", "=", 
       RowBox[{"numAsList", "[", 
        RowBox[{"[", 
         RowBox[{";;", 
          RowBox[{"Floor", "[", 
           RowBox[{
            RowBox[{"Length", "[", "numAsList", "]"}], "/", "2"}], "]"}]}], 
         "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"secondHalf", "=", 
       RowBox[{"numAsList", "[", 
        RowBox[{"[", 
         RowBox[{
          RowBox[{
           RowBox[{"Floor", "[", 
            RowBox[{
             RowBox[{"Length", "[", "numAsList", "]"}], "/", "2"}], "]"}], 
           "+", "1"}], ";;"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"x", "\[Equal]", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"ToExpression", "[", 
          RowBox[{"StringJoin", "[", "firstHalf", "]"}], "]"}], "+", 
         RowBox[{"ToExpression", "[", 
          RowBox[{"StringJoin", "[", "secondHalf", "]"}], "]"}]}], 
        ")"}]}]}]}], "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"posFiveLength", "=", 
   RowBox[{"Range", "[", 
    RowBox[{"10000", ",", "99999"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"foundKaps", "=", 
  RowBox[{"Select", "[", 
